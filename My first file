Console.WriteLine("Enter first no");
int n1 = Convert.ToInt32(Console.ReadLine());
Console.WriteLine("Enter second no");
int n2 = Convert.ToInt32(Console.ReadLine());
int sum = n1+n2;
Console.WriteLine("Sum is : "+ sum);
int sub = n1-n2;
Console.WriteLine("Sub is : "+ sub);
